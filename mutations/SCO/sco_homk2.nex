BEGIN MRBAYES;
	set autoclose=yes nowarn=yes;

	lset coding=variable rates=gamma;
	prset brlenspr=unconstrained: exp(3.78);

charset partA =2	6	8	11	14	15	16;
charset partB = 17	23	24	25	1	4	9;
charset partC =18	22	26	27	3	5	12;
charset partD =13	19	20	7	10	21;

	partition chartype=4: partA, partB, partC, partD;
	set partition=chartype;

	unlink shape=(all) pinvar=(all);
	prset applyto=(all) symdirihyperpr=fixed(infinity);
	prset applyto=(all) ratepr=variable;

	outgroup Plumariidae;

	mcmcp ngen=5000000 samplefreq=200 nruns=8 nchains=8 burninfrac=0.25 append=yes temp=0.05;
	sumt relburnin=yes burnin=0.25 nruns=8;
END;