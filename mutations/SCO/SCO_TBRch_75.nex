BEGIN MRBAYES;
	set autoclose=yes nowarn=yes;

	lset coding=variable rates=gamma;
	prset brlenspr=unconstrained: exp(4.82);

charset partA=	1	3	10	2	7	11	;	
charset partB=	12	20	26	4	8	15	;	
charset partC=	16	18	23	5	14	19	;	
charset partD=	9	25	6	17	21	22	13	24
;

	partition chartype=4: partA, partB, partC, partD;
	set partition=chartype;

	unlink shape=(all) pinvar=(all);
	prset applyto=(all) symdirihyperpr=fixed(infinity);
	prset applyto=(all) ratepr=variable;

	outgroup Plumariidae;

	mcmcp ngen=5000000 samplefreq=2000 nruns=8 nchains=8 burninfrac=0.25 append=no temp=0.05;
	mcmc;
	sumt relburnin=yes burninfrac=0.25 contype=allcompat;
END;